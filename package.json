{
  "name": "node-react-auth",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "server": "yarn --cwd server dev",
    "client": "yarn --cwd client start",
    "start": "concurrently \"yarn server\" \"yarn client\"",
    "install:server": "yarn --cwd server install",
    "install:client": "yarn --cwd client install",
    "install:all": "concurrently  \"yarn install\" \"yarn install:server\" \"yarn install:client\"",
    "eslint:check": "yarn run eslint"
  },
  "dependencies": {
    "concurrently": "^5.3.0"
  },
  "proxy": "http://localhost:3005",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "typescript": "^4.4.2"
  },
  "author": "Roman Grankov <mustbe09@yandex.ru>",
  "license": "ISC"
}
